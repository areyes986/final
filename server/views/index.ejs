<html>

<head>
  <title>Star Wars People</title>
  <link rel="stylesheet" href="/css/base.css">
  <link rel="stylesheet" href="/css/layout.css">
  <link rel="stylesheet" href="/css/module.css">
  <link href="https://fonts.googleapis.com/css?family=Poller+One&display=swap" rel="stylesheet">
<body>
  <header>
    <h1>Star Wars Characters</h1>
    <p>Vote for your favorites to show what you like!</p>
  </header>
  <main>
    <section class="characters">
      <% for(let x = 0; x < results.length; x++) { %>
      <div class="character">
        <header>

          <h2><%= results[x].name %></h2>
          <h2><%= results[x].gender %></h2>
        </header>
        <span><%= results[x].likes || 0 %></span>
        <button>Upvote</button>
      </div>
      <% } %>
    </section>
  </main>
  <button id="moreCharacters">More Characters!</button>
  <footer>
    <strong>&copy; 2020 Code Fellows</strong>
  </footer>
  <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/handlebars@latest/dist/handlebars.js"></script>
  <script src="/js/app.js"></script>
</body>

</html>
